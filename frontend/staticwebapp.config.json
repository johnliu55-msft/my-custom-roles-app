{
    "routes": [
        {
            "route": "/admin/*",
            "allowedRoles": [ "admin" ]
        },
        {
            "route": "/authenticated/*",
            "allowedRoles": [ "authenticated" ]
        }
    ],
    "auth": {
        "rolesSource": "/api/GetRoles"
    },
    "globalHeaders": {
        "Cache-Control": "no-cache"
    }
}
